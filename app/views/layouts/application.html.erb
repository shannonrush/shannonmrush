<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<%= stylesheet_link_tag 'style' %>
		<%= javascript_include_tag 'jquery-1.3.2.min','application','jquery.cycle.lite.1.0.min.js' %>
		<!--[if IE]>
		    <link rel="stylesheet" type="text/css" href="/stylesheets/iestyles.css" />
		<![endif]--> 
		<title>Shannon M Rush Photography</title>
	</head>
	<body>
		<div class="main_logo"><%= link_to image_tag('main_logo.png'), root_path %></div>
		<div class="navbar"><%= link_to "Portraits", {:controller => "portraits", :action => "index"}, :class => "navitem #{selected_status(@controller.controller_name, 'portraits')}" %><%= link_to "Real Estate", {:controller => "real_estate", :action => "index"}, :class => "navitem #{selected_status(@controller.controller_name, 'real_estate')}" %><%#= link_to_remote "Models", :update => "main_content", :url => {:controller => "portfolios", :action => "models"}, :html => {:class=>"navitem"} %><%= link_to "About", {:controller => "about", :action => "index"}, :class=>"navitem #{selected_status(@controller.controller_name, 'about')}" %><%= link_to "Contact", {:controller => "contact", :action => "index"}, :class=>"navitem #{selected_status(@controller.controller_name, 'contact')}" %><%= link_to "Galleries", {:controller => "galleries", :action => "index"}, :class => selected_status(@controller.controller_name, 'galleries') %></div>
		<div id="main_content"><%= yield %></div>
		<script type="text/javascript">
		var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
		document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
		</script>
		<script type="text/javascript">
		try {
		var pageTracker = _gat._getTracker("UA-12593174-3");
		pageTracker._trackPageview();
		} catch(err) {}</script>
	</body>
</html>
